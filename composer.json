{
    "name": "paxha/lararole",
    "description": "Laravel Role Management",
    "type": "laravel",
    "license": "MIT",
    "keywords": [
        "laravel library",
        "lararole",
        "role management",
        "user management"
    ],
    "authors": [
        {
            "name": "Hassan Raza Pasha",
            "email": "e.hassanrazapasha@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/paxha/lararole.git"
        }
    ],
    "require": {
        "php": "^7.2",
        "doctrine/dbal": "^2.10",
        "sven/artisan-view": "^3.3",
        "staudenmeir/eloquent-has-many-deep": "^1.11",
        "staudenmeir/laravel-adjacency-list": "^1.2",
        "paxha/db-truncate": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.4",
        "phpunit/phpunit": "^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lararole\\LararoleServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Lararole\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lararole\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testsuite Feature"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
