{
    "name": "paxha/lararole",
    "description": "Laravel Role Management",
    "type": "laravel",
    "license": "MIT",
    "keywords": [
        "laravel library",
        "lararole",
        "role management",
        "user management"
    ],
    "authors": [
        {
            "name": "Hassan Raza Pasha",
            "email": "e.hassanrazapasha@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/paxha/lararole"
        }
    ],
    "require": {
        "php": "^7.2",
        "doctrine/dbal": "^2.10",
        "paxha/has-many-through-deep": "^2.0",
        "paxha/laravel-adjacency-list": "^2.1",
        "sven/artisan-view": "^3.3"
    },
    "require-dev": {
        "orchestra/testbench": "^4.4",
        "phpunit/phpunit": "^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lararole\\LararoleServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Lararole\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lararole\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
